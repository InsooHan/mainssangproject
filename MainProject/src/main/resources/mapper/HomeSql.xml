<mapper namespace="fpro.data.mapper.HomeMapperInter">
	<!-- 커뮤니티 게시물 가져오기 -->
	<select id="getCommunityToMain" resultType="community">
		select * from community order by num desc
	</select>
	<!-- 이벤트 게시물 가져오기 -->
	<select id="getEventToMain" resultType="evdto">
